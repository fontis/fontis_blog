{
    "name":"fontis/blog",
    "type":"magento-module",
    "license":"Open Software License (OSL 3.0)",
    "description":"This extension provides a native blog for Magento.",
    "homepage":"http://www.fontis.com.au",
    "authors":[
        {
            "name":"Fontis",
            "email":"magento@fontis.com.au"
        }
    ],
    "extra":{
        "magento_connect":{
            "channel":"community",
            "php_min":"5.3.0",
            "php_max":"6.0.0",
            "stability":"stable",
            "license_uri":"http://opensource.org/licenses/osl-3.0.php",
            "summary":"Native blog for Magento.",
            "content":[
                {
                    "type":"magecommunity",
                    "structure":"dir",
                    "path":"Fontis/Blog"
                },
                {
                    "type":"magedesign",
                    "structure":"file",
                    "path":"adminhtml/default/default/layout/fontis/blog.xml"
                },
                {
                    "type":"magedesign",
                    "structure":"dir",
                    "path":"adminhtml/default/default/template/fontis/blog"
                },
                {
                    "type":"magedesign",
                    "structure":"file",
                    "path":"frontend/base/default/layout/fontis/blog.xml"
                },
                {
                    "type":"magedesign",
                    "structure":"dir",
                    "path":"frontend/base/default/template/fontis/blog"
                },
                {
                    "type":"mageetc",
                    "structure":"file",
                    "path":"modules/Fontis_Blog.xml"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"cs_CZ/Fontis_Blog.csv"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"de_DE/Fontis_Blog.csv"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"en_GB/Fontis_Blog.csv"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"en_US/Fontis_Blog.csv"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"fr_FR/Fontis_Blog.csv"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"cs_CZ/template/email/comment_submitted.html"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"de_DE/template/email/comment_submitted.html"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"en_GB/template/email/comment_submitted.html"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"en_US/template/email/comment_submitted.html"
                },
                {
                    "type":"magelocale",
                    "structure":"file",
                    "path":"fr_FR/template/email/comment_submitted.html"
                },
                {
                    "type":"mageskin",
                    "structure":"dir",
                    "path":"frontend/base/default/fontis/blog"
                }
            ]
        }
    }
}

